#include <stdio.h>
#include <stdlib.h>

int m = 0;
void p(char *str)
{
    printf(str);  // Vulnerable format string!
    
    if (m == 64) {
        fwrite("Wait what?!\n", 1, 12, stdout);
        system("/bin/sh");
    }
}

int main(void)
{
    char buffer[520];
    
    fgets(buffer, 512, stdin);
    p(buffer);
    return 0;
}