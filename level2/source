#include <stdio.h>
#include <stdlib.h>
#include <string.h>

void p(void)
{
    unsigned int ret_addr;
    char buffer[76];
    
    fflush(stdout);
    gets(buffer);
    
    ret_addr = __builtin_return_address(0);
    
    if ((ret_addr & 0xb0000000) == 0xb0000000) {
        printf("(%p)\n", ret_addr);
        exit(1);
    }
    
    puts(buffer);
    strdup(buffer);
}

int main(void)
{
    p();
    return 0;
}